{
  "name": "ui-hand",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ui/hand/src",
  "prefix": "ui",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/ui/hand"],
      "options": {
        "project": "libs/ui/hand/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui/hand/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui/hand/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui/hand"],
      "options": {
        "jestConfig": "libs/ui/hand/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/ui/hand/**/*.ts", "libs/ui/hand/**/*.html"]
      }
    }
  },
  "tags": []
}
